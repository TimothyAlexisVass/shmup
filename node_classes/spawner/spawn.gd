	return MAX_SPAWN_POINT[current_stage] if current_stage < MAX_SPAWN_POINT.size() else MAX_SPAWN_POINT["+"]

const SHIPS_LEFT_FOR_NEXT_WAVE = {
	# These positions are equivalents to G.CHALLENGE
	1: [0, 0, 0, 0],
	2: [0, 1, 1, 1],
	3: [0, 1, 2, 2],
	4: [0, 1, 2, 3],
	5: [1, 2, 3, 4],
	6: [2, 3, 4, 5],
	"+": [3, 4, 5, 6],
}

static func ships_left_for_next_wave(level):
	var current_stage =WAVE_TIER[level].stage
	return SHIPS_LEFT_FOR_NEXT_WAVE[current_stage] if current_stage < SHIPS_LEFT_FOR_NEXT_WAVE.size() else SHIPS_LEFT_FOR_NEXT_WAVE["+"]

enum WAVE_TYPE {
	MIRROR=1,    # spawn both sides at the same time
	ALTERNATE=2, # spawn one side, then the other
	FLOW=3       # flow back and forth between the spawn points
}

static func wave_type():
	var probability = randf()
	if probability < 0.5:
		return WAVE_TYPE.MIRROR
	elif probability < 0.8:
		return WAVE_TYPE.ALTERNATE
	else:
		return WAVE_TYPE.FLOW

enum WAVE_MODIFICATION {
	NONE=1,      # without modification
	WITH_MID=2,  # include spawn point 0
	ZIG_ZAG=3    # randomly change sign of spawn points
}

static func wave_modification():
	var probability = randf()
	if probability < 0.5:
		return WAVE_MODIFICATION.NONE
	elif probability < 0.8:
		return WAVE_MODIFICATION.WITH_MID
	else:
		return WAVE_MODIFICATION.ZIG_ZAG

static func at_points(maximum_point, type, modification):
	var spawn_at_points = [0] if modification == Spawn.WAVE_MODIFICATION.WITH_MID else []

	if type == Spawn.WAVE_TYPE.FLOW:
		var reversed = spawn_at_points.duplicate()
		for i in range(1, randi_range(2, maximum_point + 1)):
			var point = G.random_sign(i) if modification == Spawn.WAVE_MODIFICATION.ZIG_ZAG else i
			spawn_at_points = [-point] + spawn_at_points + [point]
			reversed = [point] + reversed + [-point]
		spawn_at_points += reversed
	else:
		for spawn_point in range(1, maximum_point + 1):
			if G.random_boolean():
				spawn_at_points.append(G.random_sign(spawn_point) if modification == Spawn.WAVE_MODIFICATION.ZIG_ZAG else spawn_point)
		if spawn_at_points.is_empty():
			spawn_at_points = [randi_range(1, maximum_point)]
	spawn_at_points.reverse()
	return spawn_at_points

static func points_at_opposite_side(points):
	var result = []
	for point in points:
		result.append(-point)
	return result

